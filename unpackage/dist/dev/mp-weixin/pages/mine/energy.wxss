.energy {
  width: 100%;
  padding: 24rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.energy .energy-top {
  background: url(data:image/jpeg;base64,) no-repeat;
  background-size: 100% 100%;
  width: 100%;
  height: 250rpx;
  -webkit-border-radius: 15rpx;
          border-radius: 15rpx;
  color: #FFFFFF;
  padding: 5% 15%;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.energy .energy-top .energy-top-num {
  font-size: 70rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}
.energy .energy-top .energy-top-num text {
  font-size: 30rpx;
}
.energy .energy-top .energy-top-title {
  font-size: 30rpx;
}
.energy .energy-content {
  margin-top: 20rpx;
  width: 100%;
  background-color: #FFFFFF;
  -webkit-border-radius: 15rpx;
          border-radius: 15rpx;
  padding: 24rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  padding-top: 40rpx;
}
.energy .energy-content .energy-content-title {
  padding-left: 20rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  border-left: 8rpx solid #2E3B67;
  font-size: 36rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
  line-height: 1;
}
.energy .energy-content .history-li {
  width: 100%;
  background-color: #FFFFFF;
  border-bottom: 1px solid #D3D3D3;
  padding: 24rpx 0;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.energy .energy-content .history-li .history-li-top {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin-bottom: 20rpx;
  line-height: 1;
}
.energy .energy-content .history-li .history-li-top .li-top-left {
  font-size: 32rpx;
  font-weight: bold;
  color: #444;
}
.energy .energy-content .history-li .history-li-top .li-top-right {
  font-size: 30rpx;
  font-weight: bold;
  color: #dd524d;
}
.energy .energy-content .history-li .history-li-bottom {
  color: #999;
  font-size: 26rpx;
}

