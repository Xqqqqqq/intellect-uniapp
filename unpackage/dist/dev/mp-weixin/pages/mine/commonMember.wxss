.common {
  width: 100%;
  padding: 24rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.common .common-banner {
  width: 100%;
  height: 300rpx;
  -webkit-border-radius: 15rpx;
          border-radius: 15rpx;
  overflow: hidden;
  background-color: #FFFFFF;
}
.common .common-banner image {
  width: 100%;
  height: 100%;
}
.common .member-price-detail {
  width: 100%;
  background-color: #FFFFFF;
  -webkit-border-radius: 50rpx 50rpx 0 0;
          border-radius: 50rpx 50rpx 0 0;
  padding: 60rpx 70rpx;
  padding-bottom: 10rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  margin-top: 70rpx;
  position: relative;
}
.common .member-price-detail .price-detail-title {
  -webkit-border-radius: 100rpx;
          border-radius: 100rpx;
  text-align: center;
  position: absolute;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  top: -6%;
  height: 80rpx;
  line-height: 80rpx;
  width: 280rpx;
  font-weight: bold;
  font-size: 30rpx;
}
.common .member-price-detail .price-detail-li {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin-bottom: 20rpx;
}
.common .member-price-detail .price-detail-li .detail-li-left {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  width: 60rpx;
  height: 60rpx;
  line-height: 65rpx;
  color: #FFFFFF;
  font-weight: bold;
  text-align: center;
  font-size: 30rpx;
  margin-right: 5%;
}
.common .member-price-detail .price-detail-li .detail-li-right {
  font-size: 30rpx;
  line-height: 65rpx;
}

