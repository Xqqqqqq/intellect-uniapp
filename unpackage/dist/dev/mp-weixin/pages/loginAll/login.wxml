<view class="login"><view class="login-up"><image src="../../static/img/icons/logo.png"></image><view class="login-uptext">武装大脑</view></view><view class="login-mid"><input placeholder="请输入手机号" type="number" data-event-opts="{{[['input',[['__set_model',['','tel','$event',[]]]]]]}}" value="{{tel}}" bindinput="__e"/><view class="login-mid-input"><input placeholder="请输入验证码" type="number" data-event-opts="{{[['input',[['__set_model',['','telCode','$event',[]]]]]]}}" value="{{telCode}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['clickCodemsg',['$event']]]]]}}" class="login-midsms" bindtap="__e">{{codeMsg}}</view></view><view data-event-opts="{{[['tap',[['clickLogin',['$event']]]]]}}" class="login-midbtn" bindtap="__e">登录</view></view><block wx:if="{{checked==true&&weChatPhone==''}}"><button class="login-down" open-type="getPhoneNumber" data-event-opts="{{[['getphonenumber',[['onGetPhoneNumber',['$event']]]]]}}" bindgetphonenumber="__e"><image src="../../static/img/icons/weixin.jpg"></image></button></block><block wx:if="{{checked==true&&weChatPhone!=''}}"><button data-event-opts="{{[['tap',[['clickWechatLogin',['$event']]]]]}}" class="login-down" bindtap="__e"><image src="../../static/img/icons/weixin.jpg"></image></button></block><block wx:if="{{checked==false}}"><button data-event-opts="{{[['tap',[['openProvider',['$event']]]]]}}" class="login-down" bindtap="__e"><image src="../../static/img/icons/weixin.jpg"></image></button></block><checkbox-group data-event-opts="{{[['change',[['changeCheckbox',['$event']]]]]}}" class="login-bottom" bindchange="__e"><checkbox style="transform:scale(0.7);" checked="{{checked}}"></checkbox><view data-event-opts="{{[['tap',[['gotoUrl',['/pages/loginAll/useragreement']]]]]}}" class="login-botext" bindtap="__e">武装大脑服务用户协议</view></checkbox-group></view>