.test-over {
  width: 100%;
}
.test-over .test-over-top {
  width: 100%;
  background-color: #FFFFFF;
  padding-top: 40rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.test-over .test-over-top .over-top-li {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  padding: 0 50rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.test-over .test-over-top .over-top-li .over-top-li-right {
  margin-bottom: 30rpx;
}
.test-over .test-over-top .over-top-li .over-top-li-box {
  text-align: center;
  margin-bottom: 40rpx;
}
.test-over .test-over-top .over-top-li .over-top-li-box .top-li-box-num {
  color: #333333;
  font-weight: bold;
  font-size: 44rpx;
  margin-bottom: 10rpx;
}
.test-over .test-over-top .over-top-li .over-top-li-box .top-li-box-title {
  color: #666666;
  font-size: 24rpx;
}
.test-over .test-over-top .over-top-li .over-top-li-line {
  width: 1rpx;
  height: 80rpx;
  border-left: 1rpx dashed #999999;
  margin-top: 2%;
}
.test-over .test-over-content {
  width: 100%;
  margin-top: 40rpx;
}
.test-over .test-over-content .over-content-title {
  color: #4b95e6;
  font-size: 26rpx;
  margin-bottom: 30rpx;
  text-align: center;
  font-weight: bold;
  line-height: 1.1;
  font-style: italic;
}
.test-over .test-over-content .over-content-title text {
  display: block;
  /*设置为块级元素会独占一行形成上下居中的效果*/
  font-size: 36rpx;
  color: #666;
  position: relative;
  /*定位横线（当横线的父元素）*/
}
.test-over .test-over-content .over-content-title text:before, .test-over .test-over-content .over-content-title text:after {
  content: '';
  /*CSS伪类用法*/
  position: absolute;
  /*定位背景横线的位置*/
  top: 52%;
  background: #666;
  /*宽和高做出来的背景横线*/
  width: 9%;
  height: 2px;
}
.test-over .test-over-content .over-content-title text:before {
  left: 28%;
  /*调整背景横线的左右距离*/
}
.test-over .test-over-content .over-content-title text:after {
  right: 28%;
}
.test-over .test-over-content .over-content-box {
  width: 100%;
  padding: 30rpx 40rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.test-over .test-over-content .over-content-box .content-box-li {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin-bottom: 30rpx;
}
.test-over .test-over-content .over-content-box .content-box-li .box-li-left {
  background-color: #FFFFFF;
  width: 280rpx;
  -webkit-border-radius: 15rpx;
          border-radius: 15rpx;
  overflow: hidden;
  text-align: center;
  height: 280rpx;
  line-height: 280rpx;
  font-weight: bold;
  font-size: 100rpx;
}
.test-over .test-over-content .over-content-box .content-box-li .box-li-left image {
  width: 100%;
  height: 100%;
}
.test-over .test-over-content .over-content-box .content-box-li .box-li-content {
  width: 50rpx;
  height: 50rpx;
  margin-top: 18%;
}
.test-over .test-over-content .over-content-box .content-box-li .box-li-content image {
  width: 100%;
  height: 100%;
}
.test-over .test-over-bottom {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  background-color: #FFFFFF;
  height: 100rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 999;
  line-height: 100rpx;
  color: #FFFFFF;
}
.test-over .test-over-bottom .over-bottom-li {
  width: 49.9%;
  text-align: center;
}
.test-over .test-over-bottom .bottom-li-green {
  background-color: #4cd964;
}
.test-over .test-over-bottom .bottom-li-yellow {
  background-color: #f0ad4e;
}
.test-over .test-over-bottom .bottom-li-red {
  background-color: #ED926E;
}
.test-over .test-over-bottom-clone {
  width: 100%;
  height: 100rpx;
}

