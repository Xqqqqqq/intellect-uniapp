page {
  background-color: #FFFFFF;
}
.img-test {
  width: 100%;
}
.img-test .img-test-box {
  width: 100%;
  padding: 50rpx 24rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.img-test .img-test-box .img-test-title {
  width: 100%;
  padding-left: 15rpx;
  border-left: 5rpx solid #2E3B67;
  color: #2E3B67;
  font-size: 32rpx;
  font-weight: bold;
  margin-bottom: 30rpx;
}
.img-test .img-test-box .img-test-content {
  width: 100%;
  background-color: rgba(75, 149, 230, 0.2);
  -webkit-border-radius: 15rpx;
          border-radius: 15rpx;
  padding: 25rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  color: #666666;
  line-height: 45rpx;
  font-size: 26rpx;
  margin-bottom: 40rpx;
}
.img-test .img-test-box .img-test-content text {
  color: #dd524d;
}
.img-test .img-test-box .img-test-content .test-content-line {
  height: 1rpx;
  width: 100%;
  border-bottom: 1rpx dashed #FFFFFF;
}
.img-test .img-test-box .img-test-content .test-content-picker {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 0 10rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  height: 80rpx;
  line-height: 80rpx;
}
.img-test .img-test-box .img-test-content .test-content-picker .content-picker-left {
  color: #333333;
}
.img-test .img-test-box .img-test-content .test-content-picker .content-picker-right {
  color: #2E3B67;
  font-weight: bold;
}
.img-test .img-test-box .img-test-content .img-test-ul {
  width: 100%;
}
.img-test .img-test-box .img-test-content .img-test-ul .img-test-ul-li {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  color: #666666;
  font-size: 26rpx;
  height: 70rpx;
  line-height: 70rpx;
}
.img-test .img-test-box .img-test-content .img-test-ul .img-test-ul-li view:nth-child(2n) {
  width: 20%;
}
.img-test .img-test-box .img-test-content .img-test-ul .img-test-ul-title {
  font-size: 40rpx;
  text-align: center;
  width: 100%;
  line-height: 80rpx;
  color: #333333;
  font-weight: bold;
}
.img-test .img-test-btn-clone {
  width: 100%;
  height: 50rpx;
}
.img-test .img-test-btn {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  background-color: #FFFFFF;
  height: 100rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 999;
}
.img-test .img-test-btn .test-btn-li {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #FFFFFF;
  font-size: 30rpx;
}
.img-test .img-test-btn .test-btn-li-gray {
  background-color: #D3D3D3;
  color: #666666;
}
.img-test .img-test-btn .test-btn-li-blue {
  background-color: #2E3B67;
}

