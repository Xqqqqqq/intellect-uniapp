.center {
  width: 100%;
}
.center .discover-top {
  width: 100%;
  background-color: #2E3B67;
  padding: 40rpx 24rpx;
  padding-bottom: 24rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.center .discover-top .wrap-select {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  background: whitesmoke;
  -webkit-border-radius: 34rpx;
          border-radius: 34rpx;
  position: relative;
  padding: 0 37rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  margin-bottom: 40rpx;
}
.center .discover-top .wrap-select .input-length {
  width: 90%;
  height: 100%;
  line-height: 70rpx;
  font-size: 28rpx;
}
.center .discover-top .wrap-select .input-placeholder {
  font-size: 26rpx;
  color: #A1A1A1;
}
.center .discover-top .wrap-select .search-icon {
  width: 35rpx;
  height: 35rpx;
  position: absolute;
  top: 50%;
  right: 34rpx;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  text-align: center;
  z-index: 999;
}
.center .discover-top .wrap-select .search-icon image {
  width: 90%;
  height: 90%;
  margin: auto;
  display: inline;
}
.center .wrap-content {
  padding: 24rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
